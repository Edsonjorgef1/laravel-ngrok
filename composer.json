{
    "name": "jn-jairo/laravel-ngrok",
    "description": "Share Laravel application with ngrok.",
    "keywords": [
        "jn-jairo",
        "laravel-ngrok"
    ],
    "homepage": "https://github.com/jn-jairo/laravel-ngrok",
    "license": "MIT",
    "authors": [
        {
            "name": "Jairo Correa",
            "email": "jn.j41r0@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/console": "~5.5.28|5.6.*|5.7.*|5.8.*",
        "illuminate/http": "~5.5.28|5.6.*|5.7.*|5.8.*",
        "illuminate/pagination": "~5.5.28|5.6.*|5.7.*|5.8.*",
        "illuminate/routing": "~5.5.28|5.6.*|5.7.*|5.8.*",
        "illuminate/support": "~5.5.28|5.6.*|5.7.*|5.8.*",
        "symfony/process": "^3.3|^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "JnJairo\\Laravel\\Ngrok\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JnJairo\\Laravel\\Ngrok\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "JnJairo\\Laravel\\Ngrok\\NgrokServiceProvider"
            ]
        }
    }
}
